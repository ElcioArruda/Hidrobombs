    "import itertools\n",
    "import pandas as pd\n",
    "\n",
    "def Tab_cap(capacities, forced_outage_rates):\n",
    "    \"\"\"\n",
    "    Creates a capacity outage probability table.\n",
    "\n",
    "    Args:\n",
    "        capacities: A list of generator capacities.\n",
    "        forced_outage_rates: A list of forced outage rates for each generator.\n",
    "\n",
    "    Returns:\n",
    "        A pandas DataFrame containing the capacity out and probability for each state.\n",
    "    \"\"\"\n",
    "\n",
    "    num_generators = len(capacities)\n",
    "    all_states = list(itertools.product([0, 1], repeat=num_generators))\n",
    "\n",
    "    capacity_out = []\n",
    "    probability = []\n",
    "\n",
    "    for state in all_states:\n",
    "        state_capacity_out = 0\n",
    "        state_probability = 1\n",
    "        for i in range(num_generators):\n",
    "            if state[i] == 1:  # Generator is out\n",
    "                state_capacity_out += capacities[i]\n",
    "                state_probability *= forced_outage_rates[i]\n",
    "            else:\n",
    "                state_probability *= (1 - forced_outage_rates[i])\n",
    "        capacity_out.append(state_capacity_out)\n",
    "        probability.append(state_probability)\n",
    "\n",
    "    # Create a DataFrame to store the results\n",
    "    cop_table = pd.DataFrame({'Capacity Out': capacity_out, 'Probability': probability})\n",
    "\n",
    "    # Sort the DataFrame by capacity out\n",
    "    cop_table = cop_table.sort_values(by='Capacity Out')\n",
    "\n",
    "    return cop_table\n",
    "\n",
    "# # Example usage:\n",
    "# capacities = [100, 200, 300]\n",
    "# forced_outage_rates = [0.05, 0.03, 0.02]\n",
    "\n",
    "# cop_table = create_cop_table(capacities, forced_outage_rates)\n",
    "# print(cop_table)"
   ]
