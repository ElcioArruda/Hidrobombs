    "hm = pd.read_excel(r\"\MTTR.xlsx\",\"Export\")\n",
    "nic = pd.read_excel(r\"\NIC.xlsx\",\"Export\")\n",
    "hm = hm.fillna(value=0)\n",
    "nic = nic.fillna(value=0)\n",
    "# nic.head(6)\n",
    "hm.head(5)\n",
    "#bomba = 'PCCE0057'\n",
    "Saida  = {'Bomba':[],'MTTR':[],'MTTF':[],'Tfalha':[],'Treparo':[],'P0el':[],'P1el':[]}#,'t_analise':[], 'P0t':[],'P1t':[]}\n",
    "for i in range(1, len(hm.columns)):\n",
    "    bomba = hm.columns[i]\n",
    "    # print(bomba)\n",
    "    # Criando o DataFrame para cada bomba [data, bomba, hm, nic, Hmes]\n",
    "    ana  = hm.loc[:,['ANO-MES',bomba]]\n",
    "    ana.columns = ['data','hm']\n",
    "    nic_b = nic.loc[:,[bomba]]\n",
    "    nic_b.columns = ['nic']\n",
    "    ana  = pd.concat([ana,nic_b],axis=1)\n",
    "\t\n",
    "\t# Convertendo a coluna 'data' para o tipo datetime\n",
    "    ana['data'] = pd.to_datetime(ana['data'])\n",
    "    mes = ana['data'].dt.month\n",
    "    ano = ana['data'].dt.year\n",
    "\t# print(ano)\n",
    "    num_horas = []\n",
    "\t# num_dias  = []\n",
    "    for i in range(len(mes)):\n",
    "        num_dias = calendar.monthrange(int(ano[i]),int(mes[i]))[1]\n",
    "        num_horas.append(num_dias*24)\n",
    "\t    # print(num_horas)\n",
    "    hmes = pd.DataFrame(num_horas)\n",
    "    hmes.columns = ['Hmes']\n",
    "    ana  = pd.concat([ana,hmes],axis=1)\n",
    "    ana['Hop'] = ana['Hmes'] - ana['hm']\n",
    "    # print(hm)\n",
    "    # Calculando os índices para cada bomba\n",
    "    mttr = ana['hm'].sum()/ana['nic'].sum()\n",
    "    mtbf = ana['Hop'].sum()/ana['nic'].sum()\n",
    "    mttf = mtbf - mttr\n",
    "    lamb = 1/mttf\n",
    "    mu   = 1/mttr\n",
    "    # Probabilidades de estado limite\n",
    "    P0   = mu/(mu+lamb)\n",
    "    P1   = lamb/(mu+lamb)\n",
    "    # Probabilidades dependentes do tempo\n",
    "    t    = 20\n",
    "    P0t  = P0 + P1*np.exp(-(lamb+mu)*t)\n",
    "    P1t  = P1 - P1*np.exp(-(lamb+mu)*t)\n",
    "    # inserindo os índices em uma lista\n",
    "    Saida['Bomba'].append(bomba)\n",
    "    Saida['MTTR'].append(mttr)\n",
    "    Saida['MTTF'].append(mttf)\n",
    "    Saida['Tfalha'].append(lamb)\n",
    "    Saida['Treparo'].append(mu)\n",
    "    Saida['P0el'].append(P0)\n",
    "    Saida['P1el'].append(P1)\n",
    "    #Saida['t_analise'].append(t)\n",
    "    #Saida['P0t'].append(P0t)\n",
    "    #Saida['P1t'].append(P1t)\n",
    "    # print(bomba, lamb,mu,P0t,P1t)\n",
    "    # print(Saida)\n",
    "# Criando um data frame com os dados de saída de todas as bombas.\n",
    "Sdf  = pd.DataFrame(Saida)\n",
    "print(Sdf)\n"
